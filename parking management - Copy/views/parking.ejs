<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="../css/style.css">
    </head>
<body>
    <%- include('../partials/header.ejs')%>
   
    <section id="mid">
        <div class="container">
            <div class="row">
                <%- include('../partials/left.ejs')%>
                <div class="col-md-9">
                    <h2>parking managment</h2>
                    <table class="table table-hover">
                        <tr>
                            <th>s.no</th>
                            <th>vechile no</th>
                            <th>vechile in time</th>
                            <th>vechile out time</th>
                            <th>vechile type</th>
                            <th>vechile charge amoutn</th>
                            <th>vechile park date</th>
                            <th>vechile status</th>
                            
                        </tr>
                        <%record.forEach((result,key)=>{%>
                        
                        <tr>
                            <th><%=key+1%></th>
                            <th><%=result.vno%></th>
                            <th><%=result.vin%></th>
                            <th><%=result.vout%></th>
                            <th><%=result. vtype%></th>
                            <th><%=result. amount%></th>
                            <th><%=result.parkedDate%></th>
                            <th><%=result.status%></th>
                        </tr>
                        <%})%>
                        
                    </table>
                </div>
            </div>
        </section>
       <%- include('../partials/footer.ejs')%>
    </div>
</body>
</html>